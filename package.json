    "react-scripts": "^5.0.1",
    "reactstrap": "^9.1.6",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.0.3",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.0",
    "yargs": "^17.7.1",
    "yargs-interactive": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@faker-js/faker": "^7.6.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/core-js": "^2.5.5",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/yargs-interactive": "^2.1.3",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-alloy": "^4.9.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "sass": "^1.60.0",
    "stylelint": "^15.3.0",
    "stylelint-scss": "^4.6.0",
    "ts-node": "^10.9.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*index.ts"
    ]
  }
}
